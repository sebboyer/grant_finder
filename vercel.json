{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(favicon\\.ico|style\\.css|script\\.js|.*\\.(png|jpg|jpeg|gif|svg|webp))",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ]
}

